{
    "name": "sweettooth/sweettooth-orocrm-binding-bundle",
    "type": "symfony-bundle",
    "description": "Oro CRM Sweet Tooth Bindings",
    "keywords": ["Sweet Tooth", "Bindings"],
    "homepage": "https://github.com/sweettooth/SweetToothOrocrmBindingBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "orocrm/contact-bundle": "dev-master",
        "orocrm/magento-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "target-dir": "SweetTooth/Bundle/BindingBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
